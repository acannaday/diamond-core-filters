ROOTDIR = @top_srcdir@
VPATH = @srcdir@
SRCDIR = @srcdir@
OBJROOT = @OBJROOT@

STATIC_LIBS += $(BUILD_LIB_PATH)/libsfimage.a
STATIC_LIBS += @STATIC_LIBS@


# objects used to build the searchlet(s)
FILTER_SRCS = fil_thumb.c

FILTER_OBJS = fil_thumb.o

#
# All the source files that are needed for depend.
#
SRCS = $(FILTER_SRCS)
OBJS = $(FILTER_OBJS)

#
# Other objects to cleanup
CLEAN_OTHERS +=

TARGETS = fil_thumb

SHARED_LIB += fil_thumb

include ${OBJROOT}/Makefile.template

fil_thumb: $(FILTER_OBJS)
	$(FIL_COMMAND)

default: $(TARGETS)

