VPATH = @srcdir@
SRCDIR = @srcdir@
OBJROOT = @OBJROOT@

STATIC_LIBS += $(BUILD_LIB_PATH)/libsfimage.a
STATIC_LIBS += ${BUILD_LIB_PATH}/libresults.a
STATIC_LIBS += @STATIC_LIBS@

CPPFLAGS += @GTK_CFLAGS@


# objects used to build the searchlet(s)
FILTER_SRCS = fil_img_diff.c

FILTER_OBJS =  fil_img_diff.o

#
# All the source files that are needed for depend.
#
SRCS = $(FILTER_SRCS)
OBJS = $(FILTER_OBJS)

#
# Other objects to cleanup
CLEAN_OTHERS += 

TARGETS = fil_img_diff img_diff.search

SHARED_LIB += fil_img_diff img_diff.search

include ${OBJROOT}/Makefile.template
                                                                                
fil_img_diff: $(FILTER_OBJS)
	$(FIL_COMMAND)

img_diff.search: img_diff.xml
	$(BUNDLE_COMMAND)

default: $(TARGETS)
