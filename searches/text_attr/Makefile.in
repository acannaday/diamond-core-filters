ROOTDIR = @top_srcdir@
VPATH = @srcdir@
SRCDIR = @srcdir@
OBJROOT = @OBJROOT@

# objects used to build the searchlet(s)
FILTER_SRCS = fil_text_attr.c
FILTER_OBJS = fil_text_attr.o
FILTER_LIBS += -lm

#
# All the source files that are needed for depend.
#
SRCS = $(FILTER_SRCS)
OBJS = $(FILTER_OBJS)

#
# Other objects to cleanup
CLEAN_OTHERS +=

TARGETS = fil_text_attr text_attr.search

SHARED_LIB = fil_text_attr text_attr.search

include ${OBJROOT}/Makefile.template
                                                                                
fil_text_attr: $(FILTER_OBJS)
	$(FIL_COMMAND)

text_attr.search: text_attr.xml
	$(BUNDLE_COMMAND)


default: $(TARGETS)
